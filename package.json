  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "esbuild": "^0.20.0",
    "jest": "^29.7.0",
    "prettier": "3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "enquirer": "^2.4.1",
    "ora": "5.4.1",
    "yargs": "^17.7.2"
